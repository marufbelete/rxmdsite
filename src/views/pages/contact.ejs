        <!-- Section: Contact -->
        <section data-tm-bg-img="images/pattern/p14.png">
          <div class="container">
            <div class="section-title text-center">
              <div class="row">
                <div class="col-md-8 offset-md-2">
                  <h3 class="text-uppercase mt-0 mb-50 text-theme-colored1">Contact Us</h3>
                </div>
              </div>
            </div>
            <div class="section-content">
              <div class="row">
                <div class="col-md-12">

                  <!-- Contact Form -->
                  <form id="contact_form" name="contact_form" class="contact-form-transparent" action="#" method="post">
                    <div class="row">
                      <div class="col-sm-6">
                        <div class="mb-3">
                          <label class="text-white">Name <small>*</small></label>
                          <input id="contact_form_name" name="form_name" class="form-control" type="text"
                            placeholder="Enter Name">
                        </div>
                      </div>
                      <div class="col-sm-6">
                        <div class="mb-3">
                          <label class="text-white">Email <small>*</small></label>
                          <input id="contact_form_email" name="form_email" class="form-control required email"
                            type="email" placeholder="Enter Email" required>
                          <div class="d-none" id="invalid_email" style="color: red;">
                            Email field required
                          </div>
                        </div>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-sm-6">
                        <div class="mb-3">
                          <label class="text-white">Subject <small>*</small></label>
                          <input id="contact_form_subject" name="form_subject" class="form-control required" type="text"
                            placeholder="Enter Subject">
                          <div class="d-none" id="invalid_subject" style="color: red;">
                            Subject field required.
                          </div>
                        </div>
                      </div>
                      <div class="col-sm-6">
                        <div class="mb-3">
                          <label class="text-white">Phone</label>
                          <input id="contact_form_phone" name="form_phone" class="form-control" type="text"
                            placeholder="Enter Phone">
                        </div>
                      </div>
                    </div>
                    <div class="mb-3">
                      <label class="text-white">Message</label>
                      <textarea id="contact_form_message" name="form_message" class="form-control required" rows="5"
                        placeholder="Enter Message"></textarea>
                      <div class="d-none" id="invalid_message" style="color: red;">
                        Message field required.
                      </div>
                    </div>
                    <div class="mb-3 tm-sc-button">
                      <input name="form_botcheck" class="form-control" type="hidden" value="" />
                      <button id="submit_contact_form" type="submit" class="btn btn-theme-colored1 btn-flat btn-block"
                        data-loading-text="Please wait...">
                        <span id="contact_text">Send your message</span>
                        <span id="contact_text_spin" class="d-none">
                          <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span>
                          Sending your message...
                        </span>
                      </button>
                    </div>
                  </form>
                  <div class="text-center">
                    <ul class="styled-icons icon-dark icon-lg icon-circled">
                      <li><a class="social-link" href="https://www.facebook.com/testrxmd"><i
                            class="fab fa-facebook"></i></a></li>
                      <li><a class="social-link" href="https://www.instagram.com/testrxmd/"><i
                            class="fab fa-instagram"></i></a>
                      </li>
                      <li><a class="social-link" href="mailto:info@testrxmd"><i class="fa fa-envelope"></i></a>
                      </li>
                      <li><a class="social-link" href="tel:812-296-6499"><i class="fa fa-phone"></i></a></li>
                      </li>
                      <li><a class="social-link"
                          href="https://www.google.com/maps/place/TestRx+MD/@39.2296795,-85.8986729,15z/data=!4m5!3m4!1s0x0:0x90ff532080fce25a!8m2!3d39.2296795!4d-85.8986729"><i
                            class="fab fa-google"></i></a></li>
                      </li>
                    </ul>
                  </div>
                  <!-- email sent notification -->
                  <div style="position: fixed; left: 50%; transform: translate(-50%, 0px); z-index: 9999; "
                    id="contact_form_toast" class="toast bg-dark top-50 start-50 translate-middle" role="alert"
                    aria-live="assertive" aria-atomic="true">

                    <div id="contact_form_toast" class="toast align-items-center bg-dark" role="alert"
                      aria-live="assertive" aria-atomic="true">
                      <div class="d-flex">
                        <div class="toast-body">
                          Message sent successfuly!
                        </div>
                        <button type="button" class="btn-close btn-close-white me-2 m-auto" data-bs-dismiss="toast"
                          aria-label="Close"></button>
                      </div>
                    </div>
                    <div class="alert alert-success alert-dismissible fade show" role="alert">
                      <strong>Message Sent Successfuly!</strong> We will get back with you as soon as possible. Thanks
                      for contacting TestRxMD!
                      <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
                    </div>
                  </div>
                  <!--  -->

        </section>

